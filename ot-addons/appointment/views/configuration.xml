<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_region_form" model="ir.ui.view">
        <field name="name">appointment.region.form</field>
        <field name="model">appointment.region</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="view_region_view" model="ir.ui.view">
        <field name="name">appointment.region.view</field>
        <field name="model">appointment.region</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_appointment_regions" model="ir.actions.act_window">
        <field name="name">Regions</field>
        <field name="res_model">appointment.region</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>